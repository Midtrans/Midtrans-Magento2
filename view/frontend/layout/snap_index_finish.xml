<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
      <title>Finish Page</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Thank you for your order...</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Midtrans\Snap\Block\Finish" name="snap_finish" template="Midtrans_Snap::finish.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
